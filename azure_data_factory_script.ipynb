   "source": [
    "**Code to pull data from website while accounting for 0s before month num (ex: \"09\" vs \"10\")**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "071f93e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "@concat(\n",
    "  'trip-data/green_tripdata_2024-',\n",
    "  if(\n",
    "    less(int(dataset().month), 10),\n",
    "    concat('0', string(dataset().month)),\n",
    "    string(dataset().month)\n",
    "  ),\n",
    "  '.parquet'\n",
    ")"
   ]
  }
